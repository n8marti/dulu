<%= link_to t(:See_all), events_path %>
<table class="table">
  <% if events.empty? %>
    <tr><td><%= t :No_events %></td></tr>
  <% else %>
    <% events.reverse_each do |event| %>
      <tr>
        <td class="bigger">
          <%= link_to event.name, event %>
        </td>
        <td>
          <%= t(event.domain) %>
        </td>
        <td>
          <%= event.dates_display_text %>
        </td>
      </tr>
    <% end %>
  <% end %>
</table>
