<td class="h4" id="stage-name-<%= stage.id %>">
  <%= t stage.name %>
</td>

<td id="stage-date-<%= stage.id %>">
  <%= pretty_format(stage.start_date) %>
</td>

<td>
  <%= button_tag t(:Modify_Date), class: 'btn btn-default',
                    data: {'edit-stage-id' => stage.id}  %>
</td>

<td>
  <%= button_to t(:Delete), stage_path(stage),
        method: :delete, remote: true, class: 'btn btn-default',
        data: { confirm: t(:Delete_stage_confirmation)}  %>
</td>