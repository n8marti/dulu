<%
  percent, color = activity.progress
  color = color_from_sym(color)
%>

<div class="progress">
  <div class="progress-bar" role="progressbar"
      aria-valuenow="<%= percent %>" aria-valuemin="0"
      aria-valuemax="100" style="width: <%= percent %>%; background-color: <%= color %>">
  </div>
</div>