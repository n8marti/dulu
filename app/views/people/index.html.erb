<h1>People</h1>
<p>
  <%= link_to 'Add Person', new_person_path %>
</p>
<table>
  <% @people.each do |person| %>
    <tr>
      <td><%= person.last_name %>, <%= person.first_name %></td>
      <td><%= person.email %></td>
      <td><%= link_to 'Edit', edit_person_path(person) %></td>
    </tr>
  <% end %>
</table>